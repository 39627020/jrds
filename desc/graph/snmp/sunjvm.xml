<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
	<name>SunJVM5MemoryPool</name>
	<graphName>jvmsun5pool-${label}</graphName>
