<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
    "urn:jrds:graphdesc">
<graphdesc>
    <name>SunJVM5GCSpeed2</name>
    <graphName>jvmsun5gcspeed-${label}</graphName>
    <graphTitle>JVM GC speed for ${label} on ${host}</graphTitle>
    <unit>
        <SI />
        <base>m</base>
    </unit>
    <verticalLabel>s/event</verticalLabel>
    <add>
        <name>majorcolltime</name>
        <dsName>Old Coll Time</dsName>
    </add>
    <add>
        <name>minorcolltime</name>
        <dsName>Young Coll Time</dsName>
    </add>
    <add>
        <name>majorcolcount</name>
        <dsName>Old Coll Count</dsName>
    </add>
    <add>
        <name>minorcolcount</name>
        <dsName>Young Coll Count</dsName>
    </add>
    <add>
        <rpn>majorcolcount,majorcolltime, /</rpn>
        <graphType>line</graphType>
        <legend>Duration of major collection</legend>
    </add>
    <add>
        <rpn>minorcolcount,minorcolltime, /</rpn>
        <graphType>line</graphType>
        <legend>Duration of minor collection</legend>
    </add>
    <hosttree>
        <pathelement>HOST</pathelement>
        <pathelement>SERVICES</pathelement>
        <pathstring>JVM</pathstring>
        <pathstring>${label}</pathstring>
        <pathstring>GC Speed</pathstring>
    </hosttree>
    <viewtree>
        <pathelement>SERVICES</pathelement>
        <pathstring>JVM</pathstring>
        <pathelement>HOST</pathelement>
        <pathstring>GC Speed</pathstring>
        <pathstring>${label}</pathstring>
    </viewtree>
</graphdesc>
