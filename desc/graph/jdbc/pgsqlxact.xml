<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graph
desc">
<graphdesc>
  <name>pgsqlxact</name>
  <graphName>pgsqlxact-${index}</graphName>
  <graphTitle>PostgreSQL transaction on ${index}</graphTitle>
  <verticalLabel>Transaction/s</verticalLabel>

  <add>
    <name>xact_commit</name>
    <graphType>area</graphType>
    <legend>Number of transaction committed</legend>
  </add>
  <add>
    <name>xact_rollback</name>
    <graphType>stack</graphType>
    <color>RED</color>
    <legend>Number of transaction rolled back</legend>
  </add>

  <hosttree>
    <pathelement>HOST</pathelement>
    <pathelement>SERVICES</pathelement>
    <pathelement>URL</pathelement>
    <pathstring>xact</pathstring>
  </hosttree>
  <viewtree>
    <pathelement>SERVICES</pathelement>
    <pathstring>PgSQL</pathstring>
    <pathelement>URL</pathelement>
    <pathstring>xact</pathstring>
  </viewtree>
</graphdesc>