<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
	<name>IfErrors</name>
	<graphName>iferrors-{2}</graphName>
	<graphTitle>Interface errors on interface {2} on {1}</graphTitle>
	<verticalLabel>paquets/s</verticalLabel>
	<lowerLimit>NaN</lowerLimit>
	<add>
		<legend>Upward graph</legend>
	</add>
	<add>
		<name>txerrs</name>
		<graphType>line</graphType>
		<legend>The total number of transmit errors detected by the device</legend>
	</add>
	<add>
		<name>txdrop</name>
		<graphType>line</graphType>
		<legend>The total number of packets dropped by the device</legend>
	</add>
	<add>
		<name>txfifo</name>
		<graphType>line</graphType>
		<legend>Number of FIFO buffer errors.</legend>
	</add>
	<add>
		<name>txcolls</name>
		<graphType>line</graphType>
		<legend>Number of collisions detected on the interface</legend>
	</add>
	<add>
		<name>txcarrier</name>
		<graphType>line</graphType>
		<legend>Number of carrier losses detected by the device</legend>
	</add>
	<add>
		<legend>Downward graph</legend>
	</add>
	<add>
		<name>rxerrs</name>
		<graphType>line</graphType>
		<color>green</color>
		<legend>Number of receive errors detected by the device</legend>
   	 	<reversed>true</reversed>
	</add>
	<add>
		<name>rxdrop</name>
		<graphType>line</graphType>
		<legend>Number of packets dropped by the device</legend>
	</add>
	<add>
		<name>rxfifo</name>
		<graphType>line</graphType>
		<legend>Number of FIFO buffer errors</legend>
   	 	<reversed>true</reversed>
	</add>
	<add>
		<name>rxframe</name>
		<graphType>line</graphType>
		<legend>Number of packet framing errors</legend>
   	 	<reversed>true</reversed>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>NETWORK</pathelement>
		<pathelement>INTERFACES</pathelement>
		<pathelement>INDEX</pathelement>
		<pathstring>Interface errors</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>NETWORK</pathelement>
		<pathelement>INTERFACES</pathelement>
		<pathelement>HOST</pathelement>
		<pathelement>INDEX</pathelement>
		<pathstring>Interface errors</pathstring>
	</viewtree>
</graphdesc>
