<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN"
	"urn:jrds:probedesc">
<probedesc>
	<name>LighttpdStatus</name>
	<probeName>lighttpdstatus_${index}</probeName>
	<probeClass>jrds.probe.ApacheStatus</probeClass>
	<defaultargs>
		<arg type="Integer" value="80"/>
	</defaultargs>
	<ds>
		<dsName>Total Accesses</dsName>
		<dsType>counter</dsType>
	</ds>
	<ds>
		<dsName>Total kBytes</dsName>
		<dsType>counter</dsType>
	</ds>
	<ds>
		<dsName>BusyServers</dsName>
		<dsType>gauge</dsType>
	</ds>
	<ds>
		<dsName>IdleWorkers</dsName>
		<dsType>gauge</dsType>
		<collect>IdleServers</collect>
	</ds>
	<graphs>
		<name>LighttpdRequests</name>
		<name>LighttpdTransfer</name>
		<name>LighttpdServers</name>
	</graphs>
</probedesc>
