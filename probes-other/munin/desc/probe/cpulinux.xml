<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>CpuLinuxMunin</name>
    <probeName>cpu_munin</probeName>
    <probeClass>jrds.probe.munin.Munin</probeClass>
    <specific name="fetch">cpu</specific>
    <ds>
        <dsName>user</dsName>
        <dsType>counter</dsType>
        <collect>user.value</collect>
    </ds>
    <ds>
        <dsName>nice</dsName>
        <dsType>counter</dsType>
        <collect>nice.value</collect>
    </ds>
    <ds>
        <dsName>system</dsName>
        <dsType>counter</dsType>
        <collect>system.value</collect>
    </ds>
    <ds>
        <dsName>idle</dsName>
        <dsType>counter</dsType>
        <collect>idle.value</collect>
    </ds>
    <ds>
        <dsName>iowait</dsName>
        <dsType>counter</dsType>
        <collect>iowait.value</collect>
    </ds>
    <ds>
        <dsName>irq</dsName>
        <dsType>counter</dsType>
        <collect>irq.value</collect>
    </ds>
    <ds>
        <dsName>softirq</dsName>
        <dsType>counter</dsType>
        <collect>softirq.value</collect>
    </ds>
    <graphs>
        <name>SwapIO</name>
    </graphs>
