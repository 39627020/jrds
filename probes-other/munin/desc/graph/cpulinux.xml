<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
	<name>CpuLinuxMunin</name>
	<graphName>cpu_munin</graphName>
	<graphTitle>CPU usage on ${host}</graphTitle>
	<verticalLabel>%</verticalLabel>
	<add>
		<name>user</name>
		<graphType>area</graphType>
		<legend>user</legend>
	</add>
	<add>
		<name>nice</name>
		<graphType>stack</graphType>
		<legend>nice</legend>
	</add>
	<add>
		<name>system</name>
		<graphType>stack</graphType>
		<legend>system</legend>
	</add>
	<add>
		<name>idle</name>
		<graphType>stack</graphType>
		<legend>idle</legend>
	</add>
	<add>
		<name>iowait</name>
		<graphType>stack</graphType>
		<legend>iowait</legend>
	</add>
	<add>
		<name>irq</name>
		<graphType>stack</graphType>
		<legend>irq</legend>
	</add>
	<add>
		<name>softirq</name>
		<graphType>stack</graphType>
		<legend>softirq</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathelement>LOAD</pathelement>
		<pathstring>CPU usage on ${host}</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathelement>LOAD</pathelement>
		<pathstring>CPU usage on ${host}</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>
