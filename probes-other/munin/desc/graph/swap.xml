<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
    <name>munin_swap</name>
    <graphName>swap</graphName>
    <graphTitle>Swap in/out</graphTitle>
    <verticalLabel>pages per s in (-) / out (+)</verticalLabel>
    <upperLimit>100000.0</upperLimit>
    <add>
        <name>swap_out</name>
        <graphType>line</graphType>
    </add>
    <add>
        <name>swap_in</name>
        <graphType>none</graphType>
    </add>
    <hosttree>
        <pathstring>${host}</pathstring>
        <pathstring>System</pathstring>
        <pathstring>Munin</pathstring>
        <pathstring>swap</pathstring>
    </hosttree>
    <viewtree>
        <pathstring>System</pathstring>
        <pathstring>Munin</pathstring>
        <pathstring>swap</pathstring>
        <pathstring>${host}</pathstring>
    </viewtree>
</graphdesc>
