<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
	<name>MemSolarisMunin</name>
	<graphName>memmunin</graphName>
	<graphTitle>Memory usage on ${host}</graphTitle>
	<unit>
		<binary />
	</unit>
	<verticalLabel>Bytes</verticalLabel>
	<add>
		<name>memTotalRealMB</name>
	</add>
	<add>
		<name>memUsedRealMB</name>
	</add>
	<add>
		<name>memTotalSwapMB</name>
	</add>
	<add>
		<name>memUsedSwapMB</name>
	</add>
	<add>
		<name>memAvailRealMB</name>
		<rpn>memTotalRealMB, memUsedRealMB,-</rpn>
	</add>
	<add>
		<name>memAvailSwapMB</name>
		<rpn>memTotalSwapMB, memUsedSwapMB, -</rpn>
	</add>
	<add>
		<rpn>memUsedRealMB, 1024, *, 1024, *</rpn>
		<graphType>area</graphType>
		<legend>Used Real/Physical Memory Space</legend>
	</add>
	<add>
		<rpn>memAvailRealMB, 1024, *, 1024, *</rpn>
		<graphType>stack</graphType>
		<legend>"Available Real/Physical Memory Space</legend>
	</add>
	<add>
	<rpn>memUsedSwap", "memUsedSwapMB, 1024, *, 1024, *</rpn>
	<graphType>line</graphType>
	<color>red</color>
	<legend>Used Swap Space</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>VM activity by Munin</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>Memory usage</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>
