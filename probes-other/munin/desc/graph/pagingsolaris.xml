<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
	<name>PagingSolarisMunin</name>
	<graphName>pagegingmunins</graphName>
	<graphTitle>Paging activity on ${host}</graphTitle>
	<verticalLabel>operation/s</verticalLabel>
	<add>
		<legend>Upward graph</legend>
	</add>
	<add>
		<name>pgin</name>
		<graphType>line</graphType>
		<legend>Page-in requests</legend>
	</add>
	<add>
		<name>pgpgin</name>
		<graphType>line</graphType>
		<legend>pages paged-in</legend>
	</add>
	<add>
		<legend>Downward graph</legend>
	</add>
	<add>
		<name>pgout</name>
	</add>
	<add>
		<name>scan</name>
	</add>
	<add>
		<name>pgpgout</name>
	</add>
	<add>
		<name>pgfree</name>
	</add>
	<add>
		<name>pgout</name>
		<reversed>true</reversed>
		<graphType>line</graphType>
		<legend>page-out requests</legend>
	</add>
	<add>
		<name>pgpgout</name>
		<reversed>true</reversed>
		<graphType>line</graphType>
		<legend>pages paged-out</legend>
	</add>
	<add>
		<name>scan</name>
		<reversed>true</reversed>
		<graphType>line</graphType>
		<legend>pages per second scanned</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>Paging activity</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>Paging activity</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>
