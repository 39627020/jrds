<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
	<name>SwapIO</name>
	<graphName>swapio</graphName>
	<graphTitle>Virtual Memory activity on ${host}</graphTitle>
	<verticalLabel>Pages/s</verticalLabel>
	<add>
		<name>swapIn</name>
		<graphType>line</graphType>
		<color>green</color>
		<legend>Number of pages swapped in</legend>
	</add>
	<add>
		<name>swapOut</name>
		<reversed>true</reversed>
		<graphType>line</graphType>
		<color>blue</color>
		<legend>Swap out</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>VM activity by Munin</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>VM activity by Munin</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>
