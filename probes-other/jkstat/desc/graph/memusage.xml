<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
                           "urn:jrds:graphdesc">
<graphdesc>
	<name>KstatFreeMem</name>
	<graphName>freemem</graphName>
	<graphTitle>Free memory on ${host}</graphTitle>
	<verticalLabel>bytes</verticalLabel>
	<add>
		<name>freemem</name>
	</add>
	<add>
		<rpn>freemem, 1024,*</rpn>
		<graphType>area</graphType>
		<color>GREEN</color>
		<legend>Free memory</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>Memory usage by kstat</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathelement>MEMORY</pathelement>
		<pathstring>Memory usage</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>
