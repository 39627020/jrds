<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
	<info organisation="jrds" module="main" />
	<configurations>
    	<conf name="lib" />
    	<conf name="libtest" />
     	<conf name="libjetty" />
    </configurations>
	<dependencies>
	    <!-- log4j's maven broken since 1.2.15, don't upgrade -->
		<dependency org="log4j" name="log4j" rev="1.2.14" conf="lib->default" />
  		<dependency org="org.dojotoolkit" name="dojo" rev="1.4.3" conf="lib->default" >
	    	<artifact name="dojo" type="zip" />
		</dependency>
		<dependency org="org.snmp4j" name="snmp4j" rev="1.10.1"  conf="lib->default" />
		<dependency org="org.json" name="json" rev="20090211" conf="lib->default"/>
		<dependency org="org.mortbay.jetty" name="jetty" rev="6.1.24"  conf="libjetty->default" />
		<dependency org="org.apache.xmlrpc" name="xmlrpc-server" rev="3.1.3" conf="lib->default">
		    <exclude module="junit" />
		    <exclude module="servlet-api" />
	    </dependency>
		<dependency org="commons-net" name="commons-net" rev="2.0"  conf="lib->default">
	    </dependency>
		<dependency org="mysql" name="mysql-connector-java" rev="5.1.10"  conf="lib->default" />
		<dependency org="postgresql" name="postgresql" rev="8.4-701.jdbc4"  conf="lib->default" />
		<dependency org="org.snmp4j" name="snmp4j-agent" rev="1.3.1" conf="libtest->default">
		    <exclude module="log4j" />
		    <exclude module="snmp4j" />
	    </dependency>
		<dependency org="junit" name="junit" rev="4.8.1" conf="libtest->default" />
		<dependency org="org.mortbay.jetty" name="jetty-servlet-tester" rev="6.1.24"  conf="libtest->default">
		    <exclude module="jetty" />
	    </dependency>
	</dependencies>
</ivy-module>
