<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
	<info organisation="jrds" module="main" />
	<configurations>
    	<conf name="lib" />
    	<conf name="libtest" />
     	<conf name="libjetty" />
    </configurations>
	<dependencies>
		<dependency org="log4j" name="log4j" rev="1.2.14" conf="lib->default" />
		<dependency org="mysql" name="mysql-connector-java" rev="5.1.10"  conf="lib->default" />
		<dependency org="org.snmp4j" name="snmp4j" rev="1.10.1"  conf="lib->default" />
		<dependency org="xerces" name="xercesImpl" rev="2.9.1"  conf="lib->default" />
		<dependency org="javax.mail" name="mail" rev="1.4.1"  conf="lib->default" />
  		<dependency org="org.dojotoolkit" name="dojo" rev="1.3.2" conf="lib->default" >
	    	<artifact name="dojo" type="zip" />
		</dependency>
		<dependency org="commons-net" name="commons-net" rev="2.0"  conf="lib->default" />
		<dependency org="junit" name="junit" rev="4.7" conf="libtest->default" />
		<dependency org="httpunit" name="httpunit" rev="1.6.2" conf="libtest->default">
			<exclude
				conf="rhino,js,junit,jtidy,aspectjrt,servlet-api,nekohtml,xmlParserAPIs,xercesImpl,javax.servlet" />
		</dependency>
		<dependency org="org.mortbay.jetty" name="jetty" rev="6.1.19"  conf="libjetty->default" />
		<dependency org="org.slf4j" name="slf4j-api"  rev="1.5.8" conf="libjetty->default" />
		<dependency org="org.slf4j" name="slf4j-log4j12"  rev="1.5.8" conf="libjetty->default" />
	</dependencies>
</ivy-module>
