<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
	<info organisation="jrds" module="main" />
	<configurations>
       	<conf name="build" />
    	<conf name="lib" />
    	<conf name="libtest" />
    </configurations>
	<dependencies>
		<dependency org="log4j" name="log4j" rev="1.2.14" conf="lib->default" />
		<dependency org="mysql" name="mysql-connector-java" rev="5.1.6"  conf="lib->default" />
		<dependency org="org.snmp4j" name="snmp4j" rev="1.9.1f"  conf="lib->default" />
		<dependency org="xerces" name="xercesImpl" rev="2.9.1"  conf="lib->default" />
		<dependency org="javax.mail" name="mail" rev="1.4.1"  conf="lib->default" />
  		<dependency org="org.dojotoolkit" name="dojo" rev="1.3.0b2" conf="build->default" >
	    	<artifact name="dojo" type="zip" />
		</dependency>
		<dependency org="junit" name="junit" rev="4.6" conf="libtest->default" />
		<dependency org="httpunit" name="httpunit" rev="1.6.2" conf="libtest->default">
			<exclude
				conf="rhino,js,junit,jtidy,aspectjrt,servlet-api,nekohtml,xmlParserAPIs,xercesImpl,javax.servlet" />
		</dependency>
	</dependencies>
</ivy-module>
